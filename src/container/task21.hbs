<div>
  <main>
    {{#with heading}}
      <div class='card'>
        <div class='card-body'>

          <div class='card'>
            <div class='card-body'>
              {{#with main}}
                <p class='h1'>{{title}}</p>
                <p>{{text}} </p>
                <div class='btn btn-primary'>
                  {{button_text/solid}}
                </div>
                <br />
                <br />
                <div class='btn btn-outline-primary'>
                  {{button_text/outline}}
                </div>
              {{/with}}
            </div>
          </div>

          <br />
          <div class='card'>
            <div class='card-body'>
              {{#with second}}
                <p class='h2'>{{title}}</p>
                <p>{{text}} </p>
                <div class='btn btn-secondary'>
                  {{button_text/solid}}
                </div>
                <br />
                <br />
                <div class='btn btn-outline-secondary'>
                  {{button_text/outline}}
                </div>
              {{/with}}
            </div>
          </div>

          <br />
          <div class='card'>
            <div class='card-body'>
              {{#with subblock}}
                <p class='h3'>{{title}}</p>
                <p>{{text}} </p>
                <div class='btn btn-dark'>
                  {{button_text/solid}}
                </div>
                <br />
                <br />
                <div class='btn btn-outline-dark'>
                  {{button_text/outline}}
                </div>
              {{/with}}
            </div>
          </div>

        </div>
      </div>
    {{/with}}

    <br />
    <br />
    {{#with nested_display}}
      <div class='card'>
        <div class='card-body'>

          <h1 class='display-1'>{{title}}</h1>
          <div class='card'>
            <div class='card-body'>

              {{#with children_display}}
                <h1 class='display-4'>{{title}}</h1>
                <p>{{text}} </p>
                <div class='btn btn-warning'>
                  {{button/text}}
                </div>
              {{/with}}
            </div>
          </div>

          <br />
          <p class='h6'>{{header_text}}</p>
          <div class='btn btn-info'>
            {{button/text}}
          </div>

        </div>
      </div>
    {{/with}}

    <br />
    <br />
    <div class='card'>
      <div class='card-body'>
        {{#with paragraph}}
          <p class='text-uppercase'>{{title}} </p>
          <p class='text-lowercase'>{{text}} </p>
          <a href='{{button/url}}' target='_blank'>
            <div class='btn btn-link'>
              {{button/text}}
            </div>
          </a>

          <br />
          <div class='card'>
            <div class='card-body'>
              <ul>
                {{#each list_name}}
                  <li>
                    <a href='{{url}}' target='_blank'>
                      <div class='btn btn-link'>
                        {{text}}
                      </div>
                    </a>
                  </li>
                {{/each}}
              </ul>
            </div>
          </div>

        {{/with}}
      </div>
    </div>

  </main>
</div>